import matplotlib.pyplot as plt
import numpy as np
from scipy import interpolate

x = [5.81, 5.96, 6.06, 6.20, 6.30]
y = [0.1, 0.2, 0.3, 0.5, 0.78]

x_smooth = np.linspace(min(x), max(x), 300)

cubic_interp = interpolate.interp1d(x, y, kind='cubic')
y_cubic = cubic_interp(x_smooth)

plt.figure(figsize=(12, 8))

plt.plot(x, y, 'ro', markersize=10, markerfacecolor='red')

plt.plot(x_smooth, y_cubic, 'b-', linewidth=3)
plt.xlabel('lg V', fontsize=12)
plt.ylabel('Ф, рад', fontsize=12)
plt.title('Ф(lg V)', fontsize=14)
plt.grid(True, linestyle='--', alpha=0.7)
plt.tight_layout()
plt.show()
