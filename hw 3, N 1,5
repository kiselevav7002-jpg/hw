# задача 5
import numpy as np
n=int(input())
x=[0]*n
for i in range(n):
  x[i]=int(input())
y=[0]*n
for i in range(n):
  y[i]=int(input())
def mnk(x,y):
 x = np.array(x)
 y = np.array(y)
 b = ((x*y).mean() - x.mean()*y.mean())/((x**2).mean() - x.mean()**2)
 a = y.mean() - b*x.mean()
 return a, b
print(mnk(x,y))

# задача 1
a=int(input())
b=int(input())
def gcd(a, b):

  if a==0 or b==0:
    return max(a,b)
  elif a>b:
    return gcd(a-b, b) 
  else:
    return gcd(a, b-a)
print(gcd(a,b))
